@using BlutChain.Models
@model List<TipoSanguineo>

@{
    DateTime data = ViewBag.Data;
}

<div>
    <h2>Gerenciamento de Tipos Sanguíneos</h2>

    <table class="table table-hover table-striped"
           style="margin-top:30px">
        <tr>
            <th>Tipo Sanguíneo</th>
        </tr>
        @foreach (TipoSanguineo tipoSanguineo in Model)
        {
        <tr>
            <td>@tipoSanguineo.GrupoSanguineo @tipoSanguineo.FatorRH</td>
        </tr>
        }
    </table>

    <p>Dados atualizados às: <b> @data </b></p>

</div>



